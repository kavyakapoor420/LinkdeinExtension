{
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },

  
  "include": ["src/**/*", "src"],
  "exclude": ["node_modules", "dist"]
}
